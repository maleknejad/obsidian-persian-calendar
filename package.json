  "license": "MIT",
  "devDependencies": {
    "@types/jalaali-js": "^1.2.0",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "8.51.0",
    "@typescript-eslint/parser": "8.51.0",
    "builtin-modules": "5.0.0",
    "esbuild": "0.27.2",
    "obsidian": "latest",
    "tslib": "2.8.1",
